library(testthat)
library(farviz)

test_check("farviz")

# test 1
expect_that(fars_summarize_years(2017), throws_error())

#test 2
data_1 <- fars_summarize_years(2013)$`2013`[1]
data_2 <- fars_summarize_years(2014)$`2014`[2]
expect_that(data_1 + data_2, equals(4123))
expect_that(sqrt(3) * sqrt(3), equals(3))

